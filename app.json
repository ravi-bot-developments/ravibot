{
    "name": "apm portal ticket bot",
    "description": "A apm portal ticketing discord bot!",
    "repository": "https://github.com/Jeffreyz1129/ravi",
    "env": {
        "TOKEN": {
            "description": "Your discord bot's token.",
            "required": true
        },
        "GUILD_ID": {
            "description": "The id for the server you are hosting this bot for.",
            "required": true
        },
        "PREFIX": {
            "description": "The Bot Prefix",
            "required": true
        },
        "OWNERS": {
            "description": "Comma separated user IDs of people that are allowed to use owner only commands..",
            "required": true
        },
        "PRESENCE_STATUS": { 
            "description": "The Bot Presense status.",
            "required": true

        },
        "STATUS": { 
            "description": "The Bot Game status.",
            "required": true
        }
    }
}
